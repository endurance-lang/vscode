{
   "comments": {
       "lineComment": "//",
       "blockComment": [ "/ahoy", "belay/" ]
   },
   "brackets": [
       ["{", "}"],
       ["[", "]"],
       ["(", ")"],
       ["<", ">"],
       ["⚓️", "⚓️"]
   ],
   "autoClosingPairs": [
       { "open": "{", "close": "}" },
       { "open": "[", "close": "]" },
       { "open": "(", "close": ")" },
       { "open": "<", "close": ">" },
       { "open": "\"", "close": "\"", "notIn": ["string"] },
       { "open": "/**", "close": " */", "notIn": ["string"] },
       { "open": "/*!", "close": " */", "notIn": ["string"] },
       { "open": "⚓️", "close": "⚓️"}
   ],
   "surroundingPairs": [
       ["{", "}"],
       ["[", "]"],
       ["(", ")"],
       ["<", ">"],
       ["'", "'"],
       ["\"", "\""],
       ["⚓️", "⚓️"]
   ]
}
